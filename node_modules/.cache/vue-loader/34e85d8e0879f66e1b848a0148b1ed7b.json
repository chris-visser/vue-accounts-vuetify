;AACA;AACA","file":"BaseCard.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <v-card width=\"100%\" max-width=\"450\">\n    <v-card-title primary-title>\n      <div>\n        <h2 class=\"headline mb-0\">{{title}}</h2>\n        <p>{{description}}</p>\n      </div>\n    </v-card-title>\n\n    <v-card-text>\n      <slot :is=\"component\" :status=\"status\" :submit=\"submit\" :error=\"error\" />\n    </v-card-text>\n  </v-card>\n</template>\n\n<script>\n  export default {\n    props: {\n      title: { type: String },\n      description: { type: String },\n      action: { type: String },\n      loginLink: { type: String, default: '/' },\n      loginLinkTitle: { type: String, default: 'Back to the login!' },\n    },\n    data() {\n      return {\n        error: '',\n        status: 'idle',\n      };\n    },\n\n    methods: {\n      async submit(payload) {\n\n        this.status = 'submitting';\n\n        const promise = this.$store.dispatch(this.action, payload);\n\n        if (!promise) {\n          this.status = 'error';\n          this.error = 'Unable to login. This is not your fault! :(';\n          return;\n        }\n\n        return this.$store.dispatch(this.action, payload)\n          .then(() => {\n            this.status = 'success';\n          })\n          .catch((error) => {\n            this.status = 'error';\n            this.error = error;\n          });\n      },\n    },\n  };\n</script>\n"]}]}